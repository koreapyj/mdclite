@charset "utf-8";
/* Mobile XE 가나다라*/
body{margin:0;background:#fff;color:#000;word-wrap:break-word;font-size:12pt;}
body,input,textarea,select,button,table{font-family:Tahoma, Geneva, sans-serif}
img{border:0;}
em{font-style:normal}
form{margin:0;padding:0}
form ul{margin:0;padding:0;list-style:none}
form li{margin:0 0 10px 0;padding:0 0 10px 0;border-bottom:1px dashed #999}
form li:last-child{border:0}
form p{margin:0}
form p.help{color:#666}
fieldset{border:0;margin:0;padding:0}
input,textarea,select{font-size:1em}
button{font-size:1em;cursor:pointer;background:#eee;border:1px solid #666;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;overflow:visible;padding:3px;text-shadow:1px 1px 0 #fff;box-shadow:0 0 2px #666;-moz-box-shadow:0 0 2px #666;-webkit-box-shadow:0 0 2px #666;text-decoration:none;font-weight:normal}
select{width:90%}
.pgBar { position:fixed; top:0px; width:100%; height:3px; z-index: 100; background-color:black; border-bottom: 1px solid; box-shadow:0 0 5px #333;-moz-box-shadow:0 0 5px #333;-webkit-box-shadow:0 0 5px #333;}
/* Common Class */
.fl{float:left}
.fr{float:right}
.cb{clear:both}
.al{text-align:left}
.ar{text-align:right}
.ac{text-align:center}
.big{font-size:1.2em;}
.hidden{display:none;}
.highlight{border:0 !important; padding:0 !important; background:#ff0;}
/*.googleadsbar{display:none;}*/
.fview{border:1px solid #ccc;word-wrap:break-word;}
.foot{font-size:0.8em;text-align:center;background:#EEE;padding:4px 4px;}
.itx{border:1px solid #ccc;width:90%;/*font-size:1.6em;height:30px;*/padding:5px 10px}
.itxx{border:1px solid #ccc;width:90%;font-size:1.2em;padding:5px 10px;resize:vertical}
.db{display:block}
.di{display:inline}
.fb{}
.fn{font-weight:normal}
.sn{margin:10px;*zoom:1}
.sn:after{content:"";display:block;clear:both}
.wn{background:#f00;color:#fff;padding:10px;margin:0}
.mm{text-align:left;width:100%;background:f4f4f4;border:1px solid #6c0;border-top-color:#660;border-left-color:#660;box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none;border-radius:0;-moz-border-radius:0;-webkit-border-radius:0}
.mm strong{font-size:1.6em}
.RefreshBox{border:1px solid #ccc; background-color:#eee; font-size:0.8em; text-align:center; padding: 7px; display:block; height:20px;}
/* Read */
.rd .hx{padding:10px;border:1px solid #ccc;border-left:0;border-right:0;background-color:#eee;}
.rd .hx h4{margin:0;font-size:1em}
.rd .hx em,.rd .hx span{font-size:0.75em;color:#666;margin-right:10px}
.rd .hx span{padding:0 0 0 10px;border-left:1px solid #ccc}
.rd .hx a{color:#000;text-decoration:none;}
.rd .co{margin:10px;word-break:break-word;overflow:auto}
.rd .co img{max-width:100% !important;}
.rd .tg{position:absolute;width:100%;height:100%;background:transparent;border:0;text-align:right;top:0px;left:0;font-size:1em;margin:0;box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none;border-radius:0;-moz-border-radius:0;-webkit-border-radius:0}
.rd .tgo{display:none}
.rd .tgo.open{display:block}
.rd .tgo a{color:#333}
/*.rd .ImageBox{}*/
.rd .ImageBox {max-width:99%;}
.rd .ImageBox a{border:1px solid #ccc; font-size:0.8em; text-align:center; display:block}
.rd .ImageBox img{border:0;}
.rd .Host{float:right; font-size:0.75em;}
/* Reply */
.rp{margin:0;padding:0;list-style:none}
.rp ul{margin:0;padding:10px 0 0 0}
.rp li{border-bottom:1px solid #ccc;padding:10px;overflow:hidden}
.rp li li{border-bottom:0;border-left:1px solid #ccc}
.rp li:last-child{border-bottom:1px solid #999;}
.rp p{display:inline}
.rp em,.rp span{color:#666;font-size:0.75em;margin-right:5px;}
.rp span,.rp .md{padding:0 0 0 5px;border-left:1px solid #ccc}
.rp em a{text-decoration:none}
.rp .md{font-size:1em}
.rp .block{color:#000;padding:5px;border-bottom:1px solid #999;background-color:#ddd;font-size:1em;text-align: right;}
.rp .block a{padding:0px;display:block;text-decoration:none;color:#000;*zoom:1}
.rp .DCL_blockComment{display:none;}
.rp .DCL_blockCommentView{border-bottom:1px solid #ccc;padding:10px;overflow:hidden;background-color:#eee}
/* List */
.lt,.lt ul{margin:0;padding:0;list-style:none}
.lt li{border-bottom:1px solid #999}
.lt li:first-child, .rd .StatusBar{padding:5px;padding-left:10px;padding-right:10px;border-bottom:1px solid #999;background-color:#ddd;font-size:0.75em;}
.rd .StatusBar { border-top:1px solid #999; }
.lt li:first-child a{padding:0px;display:inline;}
.lt li:first-child span{padding:0px;}
.lt li:last-child{border:0}
.lt li img:first-child{margin-right:5px;}
.lt .DCL_blockArticle{display:none;}
.lt .DCL_blockArticleView{border-bottom:1px solid #999;background-color:#ddd;}
.lt .IlbeLi{padding:3px 0 3px 10px;font-size:0.75em; overflow:hidden;}
.lt .IlbeLi a{padding:0;display:inline;}
.lt .notice{border-bottom: 0; background-color:#eee;}
.lt a{display:block;text-decoration:none;padding:10px;color:#000;*zoom:1}
.lt a:after{content:"";display:block;clear:both}
.lt a:visited{color:#666}
.lt .ad a{padding:0;}
.lt .title span{color:#333}
.lt .titlex{display:block;padding:10px;color:#000}
.lt .comment{margin-left:5px;font-size:0.75em;color:#666}
.lt .auth{margin-left:5px;font-size:0.75em;color:#666}
.lt .auth em{margin:0 5px 0 0;border-left:1px solid #ccc;padding:0 0 0 5px;color:#333}
.lt .auth span{border-left:1px solid #ccc;padding:0 0 0 5px}
.lt .th{float:left;margin:0 10px 0 0;border:1px solid #ccc;width:80px;box-shadow:0 0 3px #333;-moz-box-shadow:0 0 3px #333;-webkit-box-shadow:0 0 3px #333}
.lt p{margin:0 0 10px 0;padding:0 10px;color:#666}
.lt p a{display:inline;padding:0;}
.lt p a:after{display:none}
.lt em{color:#333}
/* Pagenation */
.pn{padding:10px 0;text-align:center;border-top:1px solid #999;font-size:0.75em}
.pn a,.pn strong{padding:5px 10px;display:inline-block;border:1px solid #ccc;text-decoration:none;background:#f8f8f8;color:#333}
.pn a:visited{color:#333}
.pn strong{border:0;background:none}
.pn .plain{padding:0 0;border:0;background:none}
/* Search Form */
.sh{padding:10px 0;text-align:center;border-top:1px solid #999;font-size:0.75em;background:#c8c8c8;}
.sh input[type=text]{width:150px;padding-top:6px;padding-bottom:6px;}
.sh input[type=submit]{font-size:1.1em; display:inline-block;text-decoration:none;}
.sh select{width:8em; }
/* Body */
.bd{position:relative;overflow-x:hidden;clear:both}
.bd a{text-decoration:none;color:#000}
.bd h2{font-size:1.5em;background:#fff;border:1px solid #aaa;font-weight:normal;}
.bd h3{font-size:1.3em;background:#eee;border:1px solid #ccc;font-weight:normal;}
.bd h2,.bd h3{position:relative;margin:-1px 0;padding:10px;border:1px solid #aaa;border-left:0;border-right:0;}
.bd h2 em,.bd h3 em{font-weight:normal;color:#777;font-size:0.5em;}
.bd h2 a,.bd h3 a{text-decoration:none;color:#000}
.bd h2 .gallLogo2{color:#5b79c9;}
.bd h2 .bn{position:absolute;top:8px;right:10px;font-size:1em;margin:0}
/* Context */
.cm{margin:0;padding:10px;background:#777;color:#fff;text-align:center;list-style:none;border-top:1px solid #555;*zoom:1}
.cm:after{content:"";display:block;clear:both}
.cm .fl{margin:0 5px 0 0}
.cm .fr{margin:0 0 0 5px}
/* Button */
.bn{display:inline-block;padding:5px 10px;font-weight:normal;font-size:0.85em;border:1px solid #333;background:#eee;color:#000;text-shadow:1px 1px 0 #fff;box-shadow:0 0 3px #333;-moz-box-shadow:0 0 3px #333;-webkit-box-shadow:0 0 3px #333;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;text-decoration:none}
.inlinebn{display:inline-block;padding:5px 10px;font-weight:normal;font-size:1em;border:1px solid #333;background:#eee;color:#000;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;text-decoration:none}
/* GNB */
.gn,.gn ul{margin:0;padding:0;list-style:none}
.gn li{margin:-1px 0;border:1px solid #999;border-left:0;border-right:0;vertical-align:top}
.gn li a{display:block;text-decoration:none;height:45px;line-height:45px;padding:0 10px;color:#000;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}
.gn li a em{font-weight:normal;color:#333;font-size:1em}
.gn li a:visited{color:#666}
.gn li li a{padding:0 10px 0 20px}
.gn li li li a{padding:0 10px 0 30px}
.gn li li li li a{padding:0 10px 0 50px}
/* Extension Vars */
.xv{background:#eee;border:1px solid #999;padding:10px}
.xv dt{margin:0;padding:0;}
.xv dd{margin:0 0 10px 0;padding:0;color:#666}
.exvar input[type=text] {border:1px solid #ccc; font-size:1.6em;height:30px; padding:5px 10px}
.exvar input[type=text].text {width:90%;}
.exvar input[type=text].email_address {width:90%;}
.exvar input[type=text].address {width:70%;}
.exvar textarea {border:1px solid #ccc;width:90%;font-size:1.6em;padding:5px 10px;height:200px;resize:vertical}
.exvar li { border:0; display:inline; margin-right: 10px; }
/* Footer */
.ft{padding:0;margin:0;list-style:none;background:#868686;overflow:hidden;zoom:1}
.ft:after{content:"";display:block;clear:both}
.ft li{margin:0;padding:0;border:1px solid #9c9a99;border-top:0;border-bottom:0;margin:0 -1px;background:#868686}
.ft a{display:inline-block;padding:0 15px;color:#fff;text-shadow:0 0 2px #000;font-size:0.75em;line-height:32px;text-decoration:none}

.button{display:inline-block;padding:5px 10px;font-weight:normal;font-size:1.1;border:1px solid #333;background:#eee;color:#000;text-shadow:1px 1px 0 #fff;box-shadow:0 0 3px #333;-moz-box-shadow:0 0 3px #333;-webkit-box-shadow:0 0 3px #333;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;text-decoration:none}
.rd .origin{padding:10px;margin:10px 10px 20px 10px;background:#ddd}

/*---- Login Form ----*/
/* Hx */
.hx{position:relative;border-bottom:1px solid #ccc8be;padding:8px 10px;margin:0}
.hx:after{content:"";margin:0 -10px;position:relative;top:10px;display:block;clear:both;height:1px;background:#fff}
.hx.h2{background:#e5e5e5;text-shadow:1px 1px 0 #fff}
.hx.h2 em{font-size:12px;color:#6352d2}
.hx.h3{background:#868686;color:#fff}
.hx.h3 em{font-size:12px}
.hx h2, .hx h3{margin:0 10px 0 0;display:inline}
.hx h2{font-size:16px;line-height:1.4}
.hx h3{font-size:14px}
.hx a{color:#000}
.hx .ex{font-size:12px}
.hx .tg{position:absolute;top:0;left:0;width:100%;height:100%;margin:0;padding:0;overflow:visible;border:0;cursor:pointer;opacity:0;background:none}
.hx .ca{font-size:12px;color:#333}
.hx .ca:after{content:"";display:inline-block;position:relative;left:4px;width:0;height:0;border:4px solid;border-color:transparent;border-left-color:#8d7de1;margin:0 -8px 0 0}
.hx .write{position:absolute;top:8px;right:10px;background-position:0 0;display:inline-block;width:28px;height:27px;font-size:0;overflow:hidden;text-indent:28px}
/* Body */
.fbd{background:#f8f8f8;padding:1px 0}
.fco{margin:10px;line-height:1.4;font-size:14px;color:#333}
.fco:after{content:"";display:block;clear:both;*zoom:1}
/* Form Field */
.ff{margin:0;padding:0}
.ff ul{margin:0;padding:0;list-style:none}
.ff li{margin:0 0 5px 0;padding:0;border-bottom:0;}
.ff img{border:0}
.ff em{font-style:normal}
.ff form{margin:0;padding:0}
.ff p{margin:14px 0 14px 0 ;}
.ff fieldset{margin:0;padding:0;border:0}
.ff textarea{resize:vertical}
.ff input[type=checkbox],input[type=radio]{width:13px;height:13px;margin:0;padding:0}
.ff label{margin-right:10px}
.ff label+input[type=text],
.ff label+input[type=tel],
.ff label+input[type=password],
.ff label+textarea,
.ff label+select{display:block;width:96%;font-size:14px;margin:0 0 5px 0}
.ff label+input[type=text],
.ff label+input[type=tel],
.ff label+input[type=password],
.ff label+textarea{padding:5px}
/* Help */
.hp{background:#eee;border-top:1px solid #ddd;padding:12px 10px !important;margin:10px -10px !important;}
.hp li{margin:0;display:inline;border-left:1px solid #ccc;padding:0 4px 0 8px;}
.hp li:first-child{border:0;}
.hp a{color:#333;text-decoration:none}
/* Button Area */
.fbna{text-align:center;padding:0 10px;margin:10px 0}
.fbna:after{content:"";display:block;clear:both}
.fbn{display:inline-block;line-height:26px !important;padding:0 10px;font-size:12px;font-weight:bold;border:1px solid;text-decoration:none;border-radius:5px;-moz-border-radius:5px;-webkit-border-radius:5px;cursor:pointer;vertical-align:middle}
.fbn[type=submit], 
.fbn[type=button]{height:28px}
.fbn[href]{height:26px}
.fbn.dark{border-color:#666;background:#777 -webkit-gradient(linear,0% 0%,0% 100%,from(#7e7c78),to(#5c5b58));background:#777 -moz-linear-gradient(top,#7e7c78,#5c5b58);background-color:#777;color:#fff;box-shadow:0 0 1px #fff inset;-moz-box-shadow:0 0 1px #fff inset;-webkit-box-shadow:0 0 1px #fff inset}
.fbn.white{border-color:#b5b5b5;background:#1b1b1b -webkit-gradient(linear,0% 0%,0% 100%,from(#fff),to(#f6f6f6),color-stop(0.5,#f0f0f0),color-stop(0.5,#e4e4e4));background:#1b1b1b -moz-linear-gradient(top,#fff,#e4e4e4);background-color:#fff;color:#000}